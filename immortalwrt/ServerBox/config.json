{
    "version": 2,
    "interval": "7s",
    "rate": "1/2m",
    "name": "RAX3000M",
    "rules": [
        {
            "type": "cpu",
            "threshold": ">=77%",
            "matcher": ""
        },
        {
            "type": "net",
            "threshold": ">=30m/s",
            "matcher": "eth0"
        },
        {
            "type": "mem",
            "threshold": "<=20%",
            "matcher": "free"
        }
    ],
    "pushes": [
        {
            "type": "ios",
            "name": "ServerBox",
            "iface": {
                "token": "{YOUR_TAKEN}",
                "title": "{{name}} 提示",
                "content": "{{msg}}",
                "body_regex": ".*",
                "code": 200
            }
        }
    ]
}